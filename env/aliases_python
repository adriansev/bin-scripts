
# Python
alias pip_install_user='python3 -m pip install --upgrade $( [[ -z ${VIRTUAL_ENV} ]] && echo "--user" )'
alias pip3_install_user=pip_install_user

alias pip_install_user_eager='python3 -m pip install --upgrade-strategy eager --upgrade $( [[ -z ${VIRTUAL_ENV} ]] && echo "--user" )'
alias pip3_install_user_eager=pip_install_user_eager


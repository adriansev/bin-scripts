cmake \
-DCMAKE_INSTALL_PREFIX=$tools/alice/physics_r6 \
-DCMAKE_C_FLAGS="-march=native -mtune=native -mfpmath=sse" -DCMAKE_CXX_FLAGS="-march=native -mtune=native -mfpmath=sse -std=c++11" \
-DCMAKE_BUILD_TYPE=RelWithDebInfo \
-DALIEN="$ALIEN_ROOT" \
-DROOTSYS=$tools/root/v6-00 \
-DALIROOT="$ALICE/v5-AN-v6" \
-DCGAL_LIB="$tools/heplibs/CGAL/lib" \
-DFASTJET="$ALICE/fastjet" \
$1 | tee cmake_out.txt

